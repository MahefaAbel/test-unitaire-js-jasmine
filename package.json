    "tslint": "^6.1.3",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  },
  "dependencies": {
    "@types/jasmine": "^3.8.2",
    "ts-node": "^10.2.1"
  }
